{
  "name": "jpreecedev",
  "version": "2.0.2",
  "description": "Official source code for JPreeceDev.com, freelancer and solutions provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpreecedev/FreelancerWebsite.git"
  },
  "author": "Jon Preece",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpreecedev/FreelancerWebsite/issues"
  },
  "homepage": "https://github.com/jpreecedev/FreelancerWebsite#readme",
  "scripts": {
    "start": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "bump": "bump --prompt",
    "build": "rimraf .cache public && gatsby build",
    "deploy": "npm run bump -- --prompt --tag --push --all && npm run build && s3-deploy './public/**' --cwd './public/' --bucket jpreecedev.com  --deleteRemoved --gzip && npm run clear",
    "clear": "aws-cloudfront-invalidate E1JZ71D11DU5CQ"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.53",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "node-sass": "^4.11.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-twitter-widgets": "^1.7.1"
  },
  "devDependencies": {
    "aws-cloudfront-invalidate": "^0.1.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-sass": "^2.0.7",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "s3-deploy": "^1.2.0",
    "stylelint": "^9.9.0",
    "stylelint-config-rational-order": "^0.0.2",
    "version-bump-prompt": "^4.2.2"
  }
}
